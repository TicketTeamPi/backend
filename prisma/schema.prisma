generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Enterprise {
  id         String   @id @default(uuid())
  name       String
  cnpj       String   @unique
  email      String?  
  phone      String?  
  created_at DateTime @default(now())
  updated_at DateTime?
}
